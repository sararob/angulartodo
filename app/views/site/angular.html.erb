<section ng-app>
  <div ng-controller=TestController>
    <h1>
      Hello, {{world}}
    </h1>

    <form ng-submit="addWords($event)">
      <input ng-model="world"/>
      <input type='submit' />
    </form>
    <div ng-repeat="item in storage">
      {{item.text}}
    </div>
  </div>
</section>

